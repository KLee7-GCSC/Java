public class Nickel {

	private double value = .05;
	private String sideUp = Toss.getToss();
	
	
	//getter to return the value of the nickel
	
	public double getValue() {
		return value;
	}
	
	public String getSideUp() {
		return sideUp;
	}//end getters
	
	public void toss() {
		sideUp = Toss.getToss();
	}
	
	
}// end class nickel
